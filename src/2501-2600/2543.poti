var entrada := leia_texto
enquanto entrada <> None.orNull faça
  n, l = entrada.divida(" ")[1].inteiro, entrada.divida(" ")[2].inteiro
  var cont := 0
  s = "{l} 0"
  para a de 1 até n faça
    linha = leia_texto
    se linha == s então
      cont := cont + 1
    fim
  fim
  escreva cont
  entrada := leia_texto
fim
